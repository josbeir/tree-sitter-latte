{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "latte",
      "camelcase": "Latte",
      "title": "Latte",
      "scope": "source.latte",
      "file-types": ["latte"],
      "injection-regex": "^latte$",
      "class-name": "TreeSitterLatte"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Latte template support",
    "authors": [
      {
        "name": "josbeir",
        "email": "josbeir@gmail.com",
        "url": "https://www.jaspersmet.be/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-latte"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
